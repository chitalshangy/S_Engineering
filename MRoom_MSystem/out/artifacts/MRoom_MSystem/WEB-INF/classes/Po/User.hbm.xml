<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Po.User" table="user"
           catalog="mroom_msystem">
        <id name="uid" type="string">
            <column name="uid" length="6" />
            <generator class="assigned" />
        </id>
        <property name="uname" type="string">
            <column name="uname" length="18" />
        </property>
        <property name="upassword" type="string">
            <column name="upassword" length="18" />
        </property>
        <property name="uphone" type="string">
            <column name="uphone" length="13" />
        </property>
        <property name="upicture" type="string">
            <column name="upicture" length="50"/>
        </property>
    </class>
</hibernate-mapping>