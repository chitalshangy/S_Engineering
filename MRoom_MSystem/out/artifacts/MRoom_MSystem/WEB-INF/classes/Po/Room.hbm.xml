<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Po.Room" table="room"
           catalog="mroom_msystem">
        <id name="rid" type="string">
            <column name="rid" length="3"/>
            <generator class="assigned"/>
        </id>
        <property name="rnum" type="int">
            <column name="rnum"/>
        </property>
        <property name="rstate" type="string">
            <column name="rstate" length="2"/>
        </property>
        <property name="raddress" type="string">
            <column name="raddress" length="100"/>
        </property>
    </class>
</hibernate-mapping>